{
  "name": "dina-ui",
  "version": "0.202.0",
  "scripts": {
    "build": "run-s next:build next:export",
    "dev": "next",
    "next": "next",
    "next:build": "node --require ./next.config.js ../../node_modules/.bin/next build",
    "next:export": "node --require ./next.config.js ../../node_modules/.bin/next export",
    "a11y-check": "pa11y-ci -j > pa11y-result.json",
    "a11y-generate-report": "pa11y-ci-reporter-html -s pa11y-result.json --destination ./pa11y-html-report"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "alphanumeric-encoder": "^1.6.1",
    "blob": "0.1.0",
    "bootstrap": "5.1.3",
    "bytes": "^3.1.1",
    "cleave.js": "^1.6.0",
    "common-ui": "link:../common-ui",
    "coordinate-parser": "^1.0.7",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.6",
    "esri-loader": "^3.3.0",
    "esri-loader-hooks": "^0.4.0",
    "moment": "^2.29.4",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dropzone-uploader": "^2.11.0",
    "react-switch": "^7.1.0",
    "react-tabs": "^6.1.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bytes": "^3.1.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
