{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "arrowParens": "avoid",
        "trailingComma": "none"
      }
    ],
    "interface-name": false,
    "jsx-no-lambda": false,
    "max-classes-per-file": false
  }
}
