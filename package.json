{
  "name": "create-next-example-app",
  "scripts": {
    "build": "next build && next export -o ./build && babel ./build -d ./build",
    "dev": "run-p next proxy",
    "docs": "cd scripts/ && ts-node ./build-docs.ts",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "next": "next",
    "proxy": "micro-proxy -r ./dev-proxy-config.json -p 8000",
    "start": "http-server -p 3000 ./build",
    "test": "tsc && jest",
    "test:coverage": "tsc && jest --coverage"
  },
  "dependencies": {
    "@molgenis/rsql": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "bootswatch": "^4.2.1",
    "core-js": "^2.6.5",
    "formik": "^1.5.1",
    "kitsu": "^6.5.1",
    "kitsu-core": "^7.0.0",
    "lodash": "^4.17.11",
    "next": "^7.0.2",
    "react": "^16.8.3",
    "react-datepicker": "^2.3.0",
    "react-dom": "^16.8.3",
    "react-select": "^2.3.0",
    "react-table": "6.8.6",
    "react-tooltip": "^3.10.0",
    "title-case": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.119",
    "@types/next": "^7.0.6",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.4",
    "@types/react-datepicker": "^2.2.0",
    "@types/react-dom": "^16.8.2",
    "@types/react-select": "^2.0.11",
    "@types/react-table": "^6.7.21",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-jsx": "^2.2.7",
    "asciidoctor.js": "^1.5.9",
    "babel-jest": "^24.3.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.0",
    "lint-staged": "^9.2.0",
    "micro-proxy": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --project tsconfig.json --fix",
      "git add"
    ],
    "*.{js,jsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
